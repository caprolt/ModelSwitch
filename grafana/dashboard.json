{
  "dashboard": {
    "id": null,
    "title": "ModelSwitch Dashboard",
    "tags": ["modelswitch", "ml", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(model_prediction_requests_total[5m])",
            "legendFormat": "{{model_version}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Inference Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_inference_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{model_version}}"
          },
          {
            "expr": "histogram_quantile(0.50, rate(model_inference_latency_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{model_version}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(model_prediction_errors_total[5m])",
            "legendFormat": "{{model_version}} - {{error_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Active Model Version",
        "type": "stat",
        "targets": [
          {
            "expr": "model_version_active",
            "legendFormat": "{{version}}"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Total Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "model_prediction_requests_total",
            "legendFormat": "{{model_version}}"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 6,
        "title": "Model Load Times",
        "type": "graph",
        "targets": [
          {
            "expr": "model_load_time_seconds",
            "legendFormat": "{{model_version}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
} 